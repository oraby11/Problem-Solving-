#include <iostream>

using namespace std;

int main()
{
    int n,k;
    cin>>n;
    cin>>k;
    int counter=0;
     int *arr = new int [n];
     for(int i=0;i<n;i++)
     {
         cin>>arr[i];
     }
     for(int i=0;i<n;i++)
     {
         if(arr[i]+k<=5)
         {
             counter++;
         }
     }
     cout<<counter/3;

      delete[] arr;

    return 0;
}
